msgid "Account lock, display configuration for login information"
msgstr "རྩིས་ཁྲ་ཟྭ་རྒྱག་པ་དང་ཐོ་འགོད་ཆ་འཕྲིན་འཆར་བ་སྒྲིག་འགོད།"

msgid "Defaults"
msgstr "སོར་བཞག"

msgid "Apply"
msgstr "ཉེར་སྤྱོད།"

msgid ""
"Update account lock and login information shows that the configuration "
"failed, please check ！"
msgstr ""
"རྩིས་ཁྲ་ཟྭ་རྒྱག་པ་རབས་བརྗེ་དང་ཐོ་འགོད་ཆ་འཕྲིན་འཆར་བ་སྒྲིག་འགོད་ལེགས་འགྲུབ་མ་བ"
"ྱུང་བས། བརྟག་དཔྱད་གནང་རོགས ！"

msgid ""
"Update account lock and login information shows that the configuration "
"failed, please check"
msgstr ""
"རྩིས་ཁྲ་ཟྭ་རྒྱག་པ་རབས་བརྗེ་དང་ཐོ་འགོད་ཆ་འཕྲིན་འཆར་བ་སྒྲིག་འགོད་ལེགས་འགྲུབ་མ་བ"
"ྱུང་བས། བརྟག་དཔྱད་གནང་རོགས།"

msgid "Account lock Settings"
msgstr "རྩིས་ཁྲ་ཟྭ་རྒྱག་སྒྲིག་འགོད།"

msgid "Enable account locking"
msgstr "རྩིས་ཁྲ་ཟྭ་རྒྱག་ནུས་པ་སྒོ་འབྱེད།"

msgid "Threshold number of consecutive password errors (3-16 times):"
msgstr "གསང་གྲངས་མུ་རུ་ནོར་ཐེངས་ཁྱབ་ཁོངས་གྲངས (ཐེངས3-16 ):"

msgid "Account lockdown time:"
msgstr "རྩིས་ཁྲ་ཟྭ་རྒྱག་དུས་ཚོད:"

msgid "Login information display (console only):"
msgstr "ཐོ་འགོད་ཆ་འཕྲིན་འཆར་བ (ཚོད་འཛིན་སྟེགས་བུ་ཁོ་ན):"

msgid "Display last login information:"
msgstr "ཐེངས་སྔོན་མའི་ཐོ་འགོད་ཆ་འཕྲིན་ཤར་བ:"

msgid "Display history login failure information:"
msgstr "ཐོ་འགོད་བྱས་ཟིན་པའི་ཕམ་ཉེས་ཆ་འཕྲིན་ཤར་བ:"

msgid "     5minute"
msgstr "     སྐར་མ་༥"

msgid "     10 minute"
msgstr "     སྐར་མ༡༠"

msgid "     30 minute"
msgstr "     སྐར་མ་༣༠"

msgid "     1hour"
msgstr "     དུས་ཚོད་གཅིག"

msgid "     3 hour"
msgstr "     དུས་ཚོད་གསུམ་"

msgid "     24 hour"
msgstr "     དུས་ཚོད་ཉེར་བཞི"

msgid "     15day"
msgstr "     ཉིན་༡༥"

msgid "     30day"
msgstr "     ཉིན་༣༠"

msgid "     60day"
msgstr "     ཉིན་༦༠"

msgid "     forever"
msgstr "     དུས་གཏན་དུ"

msgid "Acount security"
msgstr "རྩིས་ཁྲ་བདེ་འཇགས"

msgid "Protect account and login security"
msgstr "རྩིས་ཁྲ་དང་ཐོ་འགོད་ཀྱི་བདེ་འཇགས་རང་བཞིན་སྲུང་སྐྱོང་བྱེད་པ།"

msgid "Login authentication mode"
msgstr "ཐོ་འགོད་དཔང་སྤྲོད་རྣམ་པ།"

msgid "Current use : password authentication"
msgstr "མིག་སྔར་བཀོལ་བཞིན་པ་ནི་གསང་གྲངས་དཔང་སྤྲོད་ཡིན།"

msgid "Account password security"
msgstr "རྩིས་ཁྲ་གསང་གྲངས་བདེ་འཇགས"

msgid ""
"Configuration of account password strength, account locking, login "
"information display and other functions"
msgstr ""
"རྩིས་ཁྲའི་གསང་གྲངས་ཀྱི་ངར་ཚད་དང་ཟྭ་རྒྱག་པ། "
"ཐོ་འགོད་ཆ་འཕྲིན་འཆར་བ་སོགས་ཀྱི་ནུས་པ་སྒྲིག་འགོད།"

msgid "Password strength configuration"
msgstr "གསང་གྲངས་ངར་ཚད་སྒྲིག་འགོད།"

msgid ""
"The password should be at least 8 bits and contain at least 3 uppercase "
"characters, lowercase characters, Numbers and symbols"
msgstr ""
"གསང་གྲངས་ལ་མ་མཐའ་ཡང་གནས་གྲངས་༨དགོས་པ་དང་། དེར་ཡིག་ཆེན་དང་ཡིག་ཆུང་། གྲངས་ཀ། "
"མཚོན་རྟགས་བཅས་ལས་གསུམ་ཚང་དགོས།"

msgid ""
"Password must have at least 6 characters, including at least 2 of lowercase "
"letters, uppercase letters, Numbers and symbols"
msgstr ""
"གསང་གྲངས་ལ་མ་མཐའ་ཡང་གནས་གྲངས༦དགོས་པ་དང་། དེར་ཡིག་ཆེན་དང་ཡིག་ཆུང་། གྲངས་ཀ། "
"མཚོན་རྟགས་བཅས་ལས་གཉིས་ཚང་དགོས།"

msgid "Unlimited"
msgstr "ཚད་མེད་པ།"

msgid "Setting password strength according to needed"
msgstr "དགོས་མཁོ་ལྟར་གསང་གྲངས་ངར་ཚད་སྒྲིག་འགོད།"

msgid "Choose biometric authentication"
msgstr "སྐྱེ་དངོས་ངོས་འཛིན་དཔང་སྤྲོད་འདེམ་པ།"

msgid "High-level"
msgstr "མཐོ།་རིམ།"

msgid "Middle-level"
msgstr "འབྲིང་རིམ།"

msgid "Low-level"
msgstr "དམའ་རིམ།"

msgid "Self-define"
msgstr "རང་འགོད་མཚན་ཉིད།"

msgid "Account lock, login information display configuration"
msgstr "རྩིས་ཁྲའི་ཟྭ་རྒྱག་པ། ཐོ་འགོད་ཆ་འཕྲིན་འཆར་བ་སོགས་ཀྱི་ནུས་པ་སྒྲིག་འགོད།"

msgid "Password strength check function is disabled, unlimited"
msgstr "གསང་གྲངས་ངར་ཚད་ཀྱི་ལྟ་ཞིབ་ནུས་སྒོ་བརྒྱབ་ཟིན་པས། བཀག་རྒྱ་མེད།"

msgid "The minimum password length %1\n"
msgstr "གསང་གྲངས་ཀྱི་ཆེས་ཆུང་བའི་རིང་ཚད %1\n"

msgid "Password contains at least %1 characters\n"
msgstr "གསང་གྲངས་ལ་མ་མཐའ་ཡང་ཡིག་རྟགས %1་ཚུད།\n"

msgid "Password contains at least %1 uppercase characters\n"
msgstr "密码最少包含%1个大写字符 \n"

msgid "Password contains at least %1 lowercase characters\n"
msgstr "གསང་གྲངས་ལ་མ་མཐའ་ཡང་ཡིག་ཆུང་ %1་ཚུད།\n"

msgid "Password contains at least %1 numeric characters\n"
msgstr "གསང་གྲངས་ལ་མ་མཐའ་ཡང་ཨང་ཀི %1་ཚུད།\n"

msgid "Password contains at least %1 other characters\n"
msgstr "གསང་གྲངས་ལ་མ་མཐའ་ཡང་ཡིག་རྟགས་གཞན་པ%1་ཚུད།\n"

msgid "Password ban containning username\n"
msgstr "གསང་གྲངས་ལ་བཀོལ་མཁན་མིང་སྤྱད་མི་ཆོག\n"

msgid "Password prohibits the use of palindrome\n"
msgstr "གསང་གྲངས་ལ་ཕྱིར་ལྡོག་ཡི་གེ་བཀོལ་མི་ཆོག\n"

msgid "Enable check for password similarity \n"
msgstr "གསང་གྲངས་གཅིག་མཚུངས་ལ་ལྟ་བའི་སྒོ་འབྱེད་པ། \n"

msgid "Enable password dictionary\n"
msgstr "གསང་གྲངས་དག་ཡིག་སྒོ་འབྱེད\n"

msgid "Password expiry date% 1 day\n"
msgstr "གསང་གྲངས་གོ་ཆོད་དུས་ཡུན་ནི་ཉིན%1\n"

msgid "password expiry date  forever\n"
msgstr "གསང་གྲངས་གོ་ཆོད་དུས་ཡུན་ལ་ཚད་བཀག་མེད\n"

msgid "The same character can appear continuously %1 times at most\n"
msgstr "ཡིག་རྟགས་གཅིག་མཚུངས་མུ་མཐུད་འཆར་ཐེངས་ཆེས་མང་ན%1ཡིན\n"

msgid ""
"The maximum continuous sequence length of characters of the same type is "
"%1\n"
msgstr ""
"རིགས་གྲས་གཅིག་མཐུན་གྱི་ཡིག་རྟགས་རྒྱུན་མཐུད་ཕྲེང་སྟར་རིང་ཚད་ཆེས་ཆེ་བ་ནི%1\n"

msgid ""
"Characters of the same type are allowed to appear consecutively at most %1 \n"
"times \n"
msgstr ""
"རིགས་མཐུན་ཡིག་རྟགས་མུ་མཐུད་འབྱུང་ཐེངས་ཆེས་མང་ན%1\n"
"ཐེངས \n"

msgid ""
"If you fail to set a high-level password check strategy, the system will "
"continue to use the original password check strategy to protect system "
"security！"
msgstr ""
"རིམ་པ་མཐོ་བའི་གསང་གྲངས་ཞིབ་བཤེར་ཇུས་གཞི་སྒྲིག་འགོད་ལེགས་འགྲུབ་མ་བྱུང་བས། མ་ལག"
"་གིས་སྔར་བཞིན་གདོད་མི་གསང་གྲངས་ཞིབ་བཤེར་ཇུས་གཞི་བཀོལ་ནས་མ་ལག་གི་བདེ་འཇགས་སྲུང"
"་སྐྱོབ་བྱེད！"

msgid ""
"If the middle-level password check strategy fails to be set, the system will"
" continue to use the original password check strategy to protect system "
"security！"
msgstr ""
"འབྲིང་རིམ་་གསང་གྲངས་ཞིབ་བཤེར་ཇུས་གཞི་སྒྲིག་འགོད་ལེགས་འགྲུབ་མ་བྱུང་བས། མ་ལག་གི"
"ས་སྔར་བཞིན་གདོད་མི་གསང་གྲངས་ཞིབ་བཤེར་ཇུས་གཞི་བཀོལ་ནས་མ་ལག་གི་བདེ་འཇགས་སྲུང་སྐ"
"ྱོབ་བྱེད！"

msgid ""
"Setting a low-level password check strategy fails, the system will continue "
"to use the original password check strategy to protect system security！"
msgstr ""
"དམའ་རིམ་་གསང་གྲངས་ཞིབ་བཤེར་ཇུས་གཞི་སྒྲིག་འགོད་ལེགས་འགྲུབ་མ་བྱུང་བས། མ་ལག་གིས་"
"སྔར་བཞིན་གདོད་མི་གསང་གྲངས་ཞིབ་བཤེར་ཇུས་གཞི་བཀོལ་ནས་མ་ལག་གི་བདེ་འཇགས་སྲུང་སྐྱོ"
"བ་བྱེད！"

msgid "Add application network control"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་གྱི་དྲ་རྒྱའི་ཚོད་འཛིན་སྣོན་པ།"

msgid "Add application networking controls"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་གྱི་དྲ་སྦྲེལ་ཚོད་འཛིན་སྣོན་པ།"

msgid "Name (Required)"
msgstr "མིང"

msgid "Path (required)"
msgstr "ལམ་ཐིག "

msgid "Application icon"
msgstr "ཉེར་སྤྱོད་རིས་རྟགས།"

msgid "Control strategy"
msgstr "བདག་འཛིན་ཇུས་གཞི"

msgid "Cancel"
msgstr "འདོེར་བ།"

msgid "Confirm"
msgstr "གཏན་འཁེལ"

msgid "The application name cannot be empty!"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་གྱི་མིང་ནི་སྟོང་པ་ཡིན་མི་རུང་།!"

msgid "The application path cannot be empty!"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་ནི་ལམ་ཐིག་སྟོང་པ་ཡིན་མི་རུང་།!"

msgid "The application path does not exist, please add it again!"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་གྱི་ལམ་ཐིག་མེད་པས་ཡང་བསྐྱར་སྣོན་རོགས!"

msgid "The application name already exists, please re-enter!"
